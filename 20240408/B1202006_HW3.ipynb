        "year=int(input('輸入年分'))\n",
        "if year<=1582:\n",
        "  if year%4==0:\n",
        "    print('閏年')\n",
        "  else:\n",
        "    print('平年')\n",
        "else:\n",
        "  if year%4==0:\n",
        "    if year%400==0 or year%100!=0:\n",
        "      print('閏年')\n",
        "    else:\n",
        "      print('平年')\n",
        "  else:\n",
        "    print('平年')\n"
      ]
